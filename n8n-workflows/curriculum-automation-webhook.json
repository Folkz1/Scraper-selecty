        "jsonBody": "={{ JSON.stringify($json.body) }}",
        "options": {
          "timeout": 300000,
          "response": {
            "response": {
              "fullResponse": true
            }
          }
        }
      },
      "id": "execute-curriculum-scraper",
      "name": "Execute Curriculum Scraper",
